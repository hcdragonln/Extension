{
  "manifest_version": 3,
  "name": "Skip Buttons Popup",
  "version": "1.0",
  "description": "Skip button video",
  "permissions": ["storage", "scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"],
      "all_frames": true
    }
  ]
}
